{
  "name": "ElectroTab",
  "description": "A brand new modular New Tabs Replacement!",
  "version": "1.0.A",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_title": "ProtoTab",
    "default_popup": "popup.html"
  },
  "icons": {
    "256": "favicon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://prototab.me/*", "http://localhost:4200/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "newtab.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "redirect.html"
  },
  "permissions": [
    "tabs",
    "https://prototab.me/*",
    "http://localhost:4200/*"
  ],
  "author": "Eric Qian, Ben C, Vikram Peddinti",
  "homepage_url": "https://prototab.enumc.com",
  "web_accessible_resources": ["blank.png"],
  "manifest_version": 2
}
